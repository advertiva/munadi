<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.haqman.munadi</id>
    <filename>munadi</filename>
    <name>Munadi</name>
    <versionNumber>1.1.0</versionNumber>
    <description>Athan (Muslim Prayer Time) Application Calculator</description>
    <copyright>2013</copyright>

    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>

    <publisher>HaqMan</publisher>
    <permission system="true">run_native</permission>
    <permission>run_when_backgrounded</permission>
    <icon><image>res/icon.png</image></icon>

    <env var="LD_LIBRARY_PATH" value="app/native/lib:/usr/lib/qt4/lib"/>
    <arg>-style</arg>
    <arg>qnxlight</arg>
</qnx>
